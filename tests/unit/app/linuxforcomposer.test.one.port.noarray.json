{
    "name": "linuxforphp/linuxforcomposer",
    "description": "A Composer interface to run 'Linux for PHP' Docker containers.",
    "php-versions": [
        "7.0.29",
        "5.6.35"
    ],
    "modes": {
        "mode1": "detached",
        "mode2": "interactive",
        "mode3": "tty"
    },
    "ports": "8484:80",
    "volumes": {
        "volume1": "${PWD}/:/srv/www"
    },
    "script": "lfphp",
    "thread-safe": "false"
}
