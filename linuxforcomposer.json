{
    "name": "linuxforphp/linuxforcomposer",
    "description": "A Composer interface to run 'Linux for PHP' Docker containers.",
    "php-versions": [
        "7.4",
        "7.3",
        "7.2"
    ],
    "modes": {
        "mode1": "detached",
        "mode2": "interactive",
        "mode3": "tty"
    },
    "ports": {
        "port1": [
            "7474:80",
            "7373:80",
            "7272:80"
        ]
    },
    "volumes": {
        "volume1": ""
    },
    "script": [
        "echo '<?php phpinfo();' > /srv/www/index.php",
        "lfphp"
    ],
    "thread-safe": "false"
}
