{
    "name": "linuxforphp/linuxforcomposer",
    "description": "A Composer interface to run 'Linux for PHP' Docker containers.",
    "php-versions": [
        "7.2.10",
        "7.1.22",
        "7.0.31",
        "5.6.37"
    ],
    "modes": {
        "mode1": "detached",
        "mode2": "interactive",
        "mode3": "tty"
    },
    "ports": {
        "port1": [
            "8181:80",
            "8282:80",
            "8383:80",
            "8484:80"
        ]
    },
    "volumes": {
        "volume1": ""
    },
    "script": "lfphp",
    "thread-safe": "false"
}
